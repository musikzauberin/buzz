	Journal = {Pollinating Bees: The Conservation Link Between Agriculture and Nature},
	Keywords = {Meliponini - Cap2},
	Pages = {115--129},
	Title = {{Brazilian bee surveys: state of knowledge, conservation and sustainable use}},
