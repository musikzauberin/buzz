	Journal = {Oikos},
	Month = {mar},
	Number = {3},
	Pages = {243--251},
	Publisher = {Blackwell Publishing Ltd},
	Title = {{Beyond species: Why ecological interaction networks vary through space and time}},
	Url = {http://doi.wiley.com/10.1111/oik.01719},
	Volume = {124},
	Year = {2015},
