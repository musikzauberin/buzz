	Eprint = {arXiv:1208.5792v1},
	File = {:Users/jialelim/Library/Application Support/Mendeley Desktop/Downloaded/Allsopp et al. - 2008 - Valuing Insect Pollination Services with Cost of Replacement.pdf:pdf},
	Isbn = {1932-6203},
	Issn = {19326203},
