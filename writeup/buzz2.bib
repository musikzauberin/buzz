@article{Koleff2003,
	Abstract = {1. Little consensus has been reached as to general features of spatial$\backslash$nvariation in beta diversity, a fundamental component of species diversity.$\backslash$nThis could reflect a genuine lack of simple gradients in beta diversity,$\backslash$nor a lack of agreement as to just what constitutes beta diversity.$\backslash$nUnfortunately, a large number of approaches have been applied to$\backslash$nthe investigation of variation in beta diversity, which potentially$\backslash$nmakes comparisons of the findings difficult. 2. We review 24 measures$\backslash$nof beta diversity for presence/absence data (the most frequent form$\backslash$nof data to which such measures are applied) that have been employed$\backslash$nin the literature, express many of them for the first time in common$\backslash$nterms, and compare some of their basic properties. 3. Four groups$\backslash$nof measures are distinguished, with a fundamental distinction arising$\backslash$nbetween 'broad sense' measures incorporating differences in composition$\backslash$nattributable to species richness gradients, and 'narrow sense' measures$\backslash$nthat focus on compositional differences independent of such gradients.$\backslash$nOn a number of occasions on which the former have been employed in$\backslash$nthe literature the latter may have been more appropriate, and there$\backslash$nare many situations in which consideration of both kinds of measures$\backslash$nwould be valuable. 4. We particularly recommend (i) considering beta$\backslash$ndiversity measures in terms of matching/mismatching components (usually$\backslash$ndenoted a , b and c) and thereby identifying the contribution of$\backslash$ndifferent sources of variation in species composition, and (ii) the$\backslash$nuse of ternary plots to express the relationship between the values$\backslash$nof these measures and of the components, and as a way of understanding$\backslash$npatterns in beta diversity.},
	Author = {Koleff, Patricia and Gaston, Kevin J. and Lennon, Jack J.},
	Doi = {10.1046/j.1365-2656.2003.00710.x},
	Isbn = {0021-8790},
	Issn = {00218790},
	Journal = {Journal of Animal Ecology},
	Keywords = {Beta diversity,Biodiversity,Presence-absence,Similarity indices,Spatial turnover},
	Number = {3},
	Pages = {367--382},
	Pmid = {45},
	Title = {{Measuring beta diversity for presence-absence data}},
	Volume = {72},
	Year = {2003},
	Bdsk-Url-1 = {http://dx.doi.org/10.1046/j.1365-2656.2003.00710.x}}

@article{Zuur2010,
	Abstract = {1. While teaching statistics to ecologists, the lead authors of this paper have noticed common statis-tical problems. If a random sample of their work (including scientific papers) produced before doing these courses were selected, half would probably contain violations of the underlying assumptions of the statistical techniques employed. 2. Some violations have little impact on the results or ecological conclusions; yet others increase type I or type II errors, potentially resulting in wrong ecological conclusions. Most of these viola-tions can be avoided by applying better data exploration. These problems are especially trouble-some in applied ecology, where management and policy decisions are often at stake. 3. Here, we provide a protocol for data exploration; discuss current tools to detect outliers, hetero-geneity of variance, collinearity, dependence of observations, problems with interactions, double zeros in multivariate analysis, zero inflation in generalized linear modelling, and the correct type of relationships between dependent and independent variables; and provide advice on how to address these problems when they arise. We also address misconceptions about normality, and provide advice on data transformations. 4. Data exploration avoids type I and type II errors, among other problems, thereby reducing the chance of making wrong ecological conclusions and poor recommendations. It is therefore essential for good quality management and policy based on statistical analyses.},
	Archiveprefix = {arXiv},
