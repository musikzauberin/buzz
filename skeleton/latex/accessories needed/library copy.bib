doi = {10.1002/dvdy.10259},
issn = {1058-8388},
journal = {Developmental Dynamics},
keywords = {Anatomy {\&} Morphology,Developmental Biology,EVOLUTION,EXTRACELLULAR-MATRIX,FLUORESCENT,GENE-EXPRESSION,MUTANTS,MUTATIONS,PROLYL 4-HYDROXYLASE,PROTEIN,cuticle,extracellular matrix,morphogenesis,nematode,procollagen},
