author = {Murakami, S and Itoh, M T},
doi = {10.1002/neu.10255},
issn = {0022-3034},
journal = {Journal of neurobiology},
keywords = {ACHETA-DOMESTICUS,CENTRAL COMPLEX,DROSOPHILA,GRYLLUS-BIMACULATUS,LOCUST SCHISTOCERCA-GREGARIA,MALES,MATING-BEHAVIOR,Neurosciences {\&} Neurology,PHEROMONES,SEROTONIN-IMMUNOREACTIVE NEURONS,SEXUAL-BEHAVIOR,aggression,antenna,brain,courtship,serotonin},
