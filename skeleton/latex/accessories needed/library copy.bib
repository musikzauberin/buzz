journal = {Journal of Integrative Plant Biology},
keywords = {Arabidopsis,Biochemistry {\&} Molecular Biology,FLAVONOID BIOSYNTHESIS,GENE-EXPRESSION,GROWTH-RESPONSES,HYPOCOTYL ELONGATION,INHIBITION,PHOTORECEPTOR,PHYTOCHROME-B,Plant Sciences,STRESS,THALIANA,TRANSDUCTION,anthocyanin,blue light,cytokinin,hy4 mutant},
