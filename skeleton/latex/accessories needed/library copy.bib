issn = {0016-6707},
journal = {Genetica},
keywords = {ALCOHOL-DEHYDROGENASE,ANTARCTIC NOTOTHENIOID FISH,DEHYDROGENASE-RELATED GENES,DROSOPHILA-MELANOGASTER,FUNCTIONAL RETROPOSON,G-value paradox,Genetics {\&} Heredity,HUMAN Y-CHROMOSOME,INTRONLESS GENE,MOLECULAR EVOLUTION,PROCESSED PSEUDOGENES,TESTIS-SPECIFIC EXPRESSION,natural selection,new genes},
