author = {Normanno, N and {De Luca}, A and Bianco, C and Strizzi, L and Mancino, M and Maiello, M R and Carotenuto, A and {De Feo}, G and Caponigro, F and Salomon, D S},
doi = {10.1016/j.gene.2005.10.018},
