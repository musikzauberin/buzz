journal = {Development},
keywords = {AGE,C. elegans,CHEMOSENSORY NEURONS,DEVELOPMENTAL,Dauer,Developmental Biology,ENVIRONMENTAL CUES,HOMEOSTASIS,LARVAL DEVELOPMENT,NEMATODE CAENORHABDITIS-ELEGANS,NUCLEAR RECEPTOR,NUCLEOTIDE-GATED CHANNEL,Niemann-Pick type C disease,REPRODUCTIVE DEVELOPMENT,SIGNALING PATHWAY,cholesterol,hormone},
