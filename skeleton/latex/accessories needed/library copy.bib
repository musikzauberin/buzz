author = {Begun, D J},
issn = {0016-6731},
journal = {Genetics},
keywords = {ADH LOCUS,Genetics {\&} Heredity,MELANOGASTER,MITOCHONDRIAL,MOLECULAR EVOLUTION,PROTEIN EVOLUTION,SEQUENCES,SITES},
month = {feb},
number = {2},
pages = {375--382},
publisher = {GENETICS},
title = {{Origin and evolution of a new gene descended from alcohol dehydrogenase in Drosophila}},
volume = {145},
year = {1997}
}
