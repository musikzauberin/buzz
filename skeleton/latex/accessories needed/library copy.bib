number = {27},
pages = {10211--10216},
pmid = {15220473},
title = {{A probable Na+(K+)/H+ exchanger on the chloroplast envelope functions in pH homeostasis and chloroplast development in Arabidopsis thaliana.}},
volume = {101},
year = {2004}
}
