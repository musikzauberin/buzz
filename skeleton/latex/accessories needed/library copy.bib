author = {CHRISTIE, P J and ALFENITO, M R and WALBOT, V},
doi = {10.1007/BF00714468},
issn = {0032-0935},
journal = {Planta},
keywords = {ACCUMULATION,ANTHOCYANIN,COLD STRESS,COORDINATE,EXPRESSION,GENETIC-REGULATION,LOCUS,MESSENGER-RNA,METABOLISM,MOLECULAR-CLONING,PETUNIA-HYBRIDA,Plant Sciences,ZEA MAYS,ZEA-MAYS},
