journal = {Functional Ecology},
number = {4},
pages = {458--463},
pmid = {824},
title = {{Size-Dependent Predation by Dugesia lugubris (Turbellaria) on Physa acuta (Gastropoda): Experiments and Model}},
url = {http://www.jstor.org/stable/2390069},
volume = {8},
