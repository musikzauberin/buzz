keywords = {Apoptosis,Ferritin,Free radical,Iron homeostasis,Mitochondria,Oxidative damage},
number = {4},
pages = {457--463},
title = {{Ferritin, iron homeostasis, and oxidative damage}},
volume = {33},
year = {2002}
}
