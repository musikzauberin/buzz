author = {Radakovits, Randor and Jinkerson, Robert E and Darzins, Al and Posewitz, Matthew C},
doi = {10.1128/EC.00364-09},
issn = {1535-9778},
journal = {Eukaryotic Cell},
keywords = {ACID BETA-OXIDATION,ADP-GLUCOSE PYROPHOSPHORYLASE,BINDING CASSETTE TRANSPORTER,CARRIER PROTEIN,CHLOROPHYLL ANTENNA SIZE,CYANIDIOSCHYZON-MEROLAE 10D,DIATOM PHAEODACTYLUM-TRICORNUTUM,EXPRESSED SEQUENCE TAGS,Microbiology,SEED OIL CONTENT,STABLE NUCLEAR TRANSFORMATION,THIOESTERASE},
