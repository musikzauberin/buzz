journal = {Mechanisms of Development},
keywords = {Chromosome mapping,Fluorescence in situ hybridization,Germ cells,Germ stem cell,In situ hybridization,Mouse,PAZ domain,Piwi domain,Primordial germ cell,RNA binding protein,RT-PCR,Spermatogenesis,Stem cell,Testis,cDNA cloning,eIF2C,mili,miwi,piwi,rde-1,zwille},
month = {oct},
