author = {Zhang, Bin and Hu, Zongli and Zhang, Yanjie and Li, Yali and Zhou, Shuang and Chen, Guoping},
doi = {10.1007/s00299-011-1162-3},
issn = {0721-7714},
journal = {Plant Cell Reports},
keywords = {ACTIVATION,APPLE SKIN,ARABIDOPSIS,Anthocyanin,BIOCHEMISTRY,BoPAP1,EXPRESSION,FAMILY,GENE,Low temperature,PIGMENTATION,Plant Sciences,Purple kale,RED COLORATION,SEEDLINGS,Transcriptional regulation},
month = {feb},
