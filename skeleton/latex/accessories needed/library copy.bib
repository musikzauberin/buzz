doi = {10.1086/665999},
issn = {0003-0147},
journal = {American Naturalist},
keywords = {BIODIVERSITY,ECOLOGICAL COMMUNITIES,EVOLUTIONARY HISTORY,Environmental Sciences {\&} Ecology,Evolutionary Biology,NICHE CONSERVATISM,PATTERNS,PRODUCTIVITY,RELATEDNESS,RELATIVE ABUNDANCE,TAXONOMIC DISTINCTNESS,TREE,community assembly,community phylogenetic diversity,niche differentiation,phylogenetic relatedness,rare species,species abundance distribution},
