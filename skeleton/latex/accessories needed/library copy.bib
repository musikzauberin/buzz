doi = {10.1080/10408347.2011.632314},
isbn = {1040-8347},
issn = {1040-8347},
journal = {Critical Reviews in Analytical Chemistry},
keywords = {BIOLOGICAL-ACTIVITIES,Chemistry,EXCHANGE REVERSED-PHASE,FLAVONOID BIOSYNTHETIC-PATHWAY,MASS-SPECTROMETRY,MEDITERRANEAN DIET,NATURAL COLORANTS,PERFORMANCE LIQUID-CHROMATOGRAPHY,PHENOLIC-COMPOUNDS,PLANT PIGMENTS,RED WINE ANTHOCYANINS,anthocyanin intake,anthocyanins,chemical structure,color,short history},
number = {2},
