issn = {0016-6731},
journal = {Genetics},
keywords = {C-ELEGANS,CELL LINEAGES,DEVELOPMENTAL SWITCH,DIFFERENTIATION,ENCODES,EXPRESSION,Genetics {\&} Heredity,LIN-14,MUTATIONS,NEMATODE,SEQUENCES},
