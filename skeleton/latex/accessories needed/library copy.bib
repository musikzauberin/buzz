publisher = {BLACKWELL SCIENCE LTD},
title = {{Socio-economic and dietary influences on leg length and trunk length in childhood: a reanalysis of the Carnegie (Boyd Orr) survey of diet and health in prewar Britain (1937-39)}},
volume = {12},
year = {1998}
}
@article{Beck1998,
abstract = {ABSTRACT: Ecologists have had little success in the development of a synthetic understanding of the effects of hab{\~{}}tat structure on species, because structural complexity 1s measured differently in most studies and habitats. There were 3 main objectives of this study: (1) to measure and compare structural complexity between rocky intertidal and mangrove habitats, (2) to examine whether structural com- plexity affected the density, richness and size of gastropods in these habitats, and (3) to determine whether one lndex of structural complexity [e.g. fractal dimension (D) and chain-and-tape] best repre- sented features of the habltat that affected gastropods I used photogrammetric techn{\~{}}ques to measure and to compare the effects of structural complexity in quadrats (1 m apart) nested withln sites (10 m apart), shores (>l km apart) and habitats (rocky intertidal and mangrove) in Botany Bay. Australia. All indices showed that complexity was different between quadrats just meters apart in both habitats and was greater in mangrove than in rocky intertidal hab{\~{}}tats. Two lines of evidence indicated that varia- tion in complexity affected the density of gastropods In rocky intertidal but not in mangrove habitats. First, the density of gastropods varied 4-fold between quadrats withln habitats and, after gastropods were expenmentally removed, a similar density and size distribut{\~{}}on of gastropods recolon{\~{}}zed quadrats in the rocky intertidal. Second, this density was correlated with structural complexity. D was most often correlated with density and thus best represented features of the habitat that affected gas- tropods. The measurement and effects of structural complexity can be compared between habitats, and in which habitat structure may exert strong effects on the cond{\~{}}tions these compa{\~{}}isons help eluc{\~{}}date species. KEY},
author = {Beck, Michael W.},
doi = {10.3354/meps169165},
isbn = {0171-8630},
issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Field experiment,Fractal dimension,Habitat structure,Mangrove,Rocky intertidal},
pages = {165--178},
title = {{Comparison of the measurement and effects of habitat structure on gastropods in rocky intertidal and mangrove habitats}},
