doi = {10.2108/zsj.23.863},
issn = {0289-0003},
journal = {Zoological Science},
keywords = {AGGRESSIVE,BEHAVIORS,COURTSHIP,DOMINANCE HIERARCHIES,FIELD CRICKET,FIGHT,INDIVIDUAL RECOGNITION,NITRIC-OXIDE,OCTOPAMINE,Zoology,aggressive behaviour,avoidance behaviour,dominance,insect,subordinate individual},
