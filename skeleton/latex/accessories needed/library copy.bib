@article{Shackleton2005,
abstract = {Females are generally assumed to prefer larger, more dominant males. However, a growing number of studies that control for male-male competition have shown no correlation between dominance and attractiveness. Aggressive males can interfere with female mate preference either by physically coercing females into mating or by driving submissive males away and restricting mate choice. The most common method of assessing female mate choice is by using simultaneous two-choice tests. These control for male-male interactions, but usually interfere with physical and chemical cues involved in mate selection or alter male behaviour. They are therefore unsuitable for many study species, especially insects. Another method is the no-choice test that measures a females latency to mating when placed with a single male as an indication of male attractiveness. No-choice tests control for male-male aggression while allowing full contact between pairs (they allow actual mating to be directly observed rather than to occur based on a correlated behaviour). So far, however, no study has confirmed that males that entice females to mate sooner actually enjoy increased longer-term mating success. As such, the accuracy of no-choice tests as a method of examining mate choice remains untested. Here, we used no-choice tests on the black field cricket, Teleogryllus commodus, to show that (1) females did not prefer males that won fights ("dominant" males), and (2) latency to mating predicts actual mating success. We have clearly demonstrated the usefulness of no-choice tests and, considering the advantages of this method, they should be more often considered for a wider variety of taxa.},
address = {Australian Natl Univ, Sch Bot {\&} Zool, Canberra, ACT 0200, Australia. Univ New S Wales, Sch Biol Earth {\&} Environm Sci, Sydney, NSW 2052, Australia.; Shackleton, MA (reprint author), Australian Natl Univ, Sch Bot {\&} Zool, Canberra, ACT 0200, Australia.; mich},
annote = {PT: J; NR: 67; TC: 86; J9: BEHAV ECOL SOCIOBIOL; PG: 8; GA: 919TL; UT: WOS:000228640400001},
author = {Shackleton, M A and Jennions, M D and Hunt, J},
doi = {10.1007/s00265-004-0907-1},
issn = {0340-5443},
journal = {Behavioral Ecology and Sociobiology},
keywords = {ACHETA-DOMESTICUS,Behavioral Sciences,DROSOPHILA-MELANOGASTER,Environmental Sciences {\&} Ecology,FEMALE MATE CHOICE,GRYLLUS-BIMACULATUS DEGEER,MALE BODY-SIZE,MALE-MALE COMPETITION,OFFSPRING FITNESS,REPRODUCTIVE SUCCESS,SELECTION,SEXUAL,WALKER ORTHOPTERA,Zoology,male fighting ability,mate choice,no-choice tests,sexual selection,teleogryllus commodus},
month = {may},
