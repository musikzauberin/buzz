author = {Henry, Amelia and Chopra, Surinder and Clark, David G and Lynch, Jonathan P},
doi = {10.1071/FP11256},
issn = {1445-4408},
journal = {Functional Plant Biology},
keywords = {ACCUMULATION,ARABIDOPSIS,DEFICIENCY,LEAVES,LIGHT,PROTECTION,Plant Sciences,RED,STRESS,SWISS ARMY KNIFE,TEMPERATURE,coleus,maize,phosphorus stress,photosynthesis},
number = {3},
