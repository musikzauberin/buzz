keywords = {A-SITE,AMINOACYL-TRANSFER-RNA,BINDING,CONFORMATIONAL SWITCH,CROSS-LINKING,ESCHERICHIA-COLI RIBOSOME,MUTATIONS,P-SITE,PEPTIDYL-TRANSFER-RNA,STREPTOMYCIN,Science {\&} Technology - Other Topics},
month = {sep},
