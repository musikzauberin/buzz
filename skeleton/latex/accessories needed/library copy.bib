journal = {Molecular biology of the cell},
number = {2},
pages = {1018--1032},
pmid = {16339080},
title = {{Role of the Hog1 stress-activated protein kinase in the global transcriptional response to stress in the fungal pathogen Candida albicans.}},
volume = {17},
year = {2006}
}
