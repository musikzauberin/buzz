keywords = {Algorithms,Brain,Brain: metabolism,Cell Line,Complementary,Complementary: metabolism,DNA,Humans,Polymerase Chain Reaction,Polymerase Chain Reaction: methods,Reverse Transcriptase Polymerase Chain Reaction,Time Factors},
number = {4},
