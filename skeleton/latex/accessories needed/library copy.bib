issn = {1386-0291},
journal = {Clinical hemorheology and microcirculation},
keywords = {ACTIVATED PROTEIN-KINASES,ANTIINFLAMMATORY ACTIVITY,Antibiotics,BOWEL-DISEASE,BRONCHIAL EPITHELIAL-CELLS,CHEMICALLY-MODIFIED TETRACYCLINE,CYSTIC-FIBROSIS,Cardiovascular System {\&} Cardiology,HUMAN POLYMORPHONUCLEAR LEUKOCYTES,Hematology,IN-VITRO,MACROLIDE ANTIBIOTICS,SEPTIC SHOCK,immune system,inflammation,leukocyte recruitment,microcirculation},
