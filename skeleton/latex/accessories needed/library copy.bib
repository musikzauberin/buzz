author = {Thomson, T M and Lozano, J J and Loukili, N and Carrio, R and Serras, F and Cormand, B and Valeri, M and Diaz, V M and Abril, J and Burset, M and Merino, J and Macaya, A and Corominas, M and Guigo, R},
doi = {10.1101/gr.GR-1405R},
issn = {1088-9051},
