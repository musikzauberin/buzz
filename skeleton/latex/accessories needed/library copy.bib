keywords = {Biochemistry {\&} Molecular Biology,Biotechnology {\&} Applied Microbiology,DOMAIN,DROSOPHILA,EVOLUTION,GENOMIC DNA,Genetics {\&} Heredity,INTRONS,MAMMALIAN-CELLS,MESSENGER-RNA,ORIGIN,PROTEIN,SEQUENCE},
