journal = {Cell},
keywords = {BETA-GLOBIN GENE,Biochemistry {\&} Molecular Biology,Cell Biology,EPITHELIUM,FLUORESCENCE,LOCUS,MOUSE,MULTIGENE FAMILY,ODORANT RECEPTORS,REGION,REPLICATION TIME,X-CHROMOSOME},
