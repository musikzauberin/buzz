issn = {0964-6906},
journal = {Human molecular genetics},
keywords = {Biochemistry {\&} Molecular Biology,DOMAINS,EXPRESSION,FAMILY,GENOMIC STRUCTURE,Genetics {\&} Heredity,HUMAN-CHROMOSOMES,INACTIVATION,MEMBERS,PROTEINS,SEQUENCE,TELOMERE},
