@article{Kaur2015,
	Abstract = {CRISPR system is a powerful defense mechanism in bacteria and archaea to provide immunity against viruses. Recently, this process found a new application in intended targeting of the genomes. CRISPR-mediated genome editing is performed by two main components namely single guide RNA and Cas9 protein. Despite the enormous data generated in this area, there is a dearth of high throughput resource. Therefore, we have developed CrisprGE, a central hub of CRISPR/Cas-based genome editing. Presently, this database holds a total of 4680 entries of 223 unique genes from 32 model and other organisms. It encompasses information about the organism, gene, target gene sequences, genetic modification, modifications length, genome editing efficiency, cell line, assay, etc. This depository is developed using the open source LAMP (Linux Apache MYSQL PHP) server. User-friendly browsing, searching facility is integrated for easy data retrieval. It also includes useful tools like BLAST CrisprGE, BLAST NTdb and CRISPR Mapper. Considering potential utilities of CRISPR in the vast area of biology and therapeutics, we foresee this platform as an assistance to accelerate research in the burgeoning field of genome engineering.Database URL: http://crdd.osdd.net/servers/crisprge/.},
	Author = {Kaur, Karambir and Tandon, Himani and Gupta, Amit Kumar and Kumar, Manoj},
	Doi = {10.1093/database/bav055},
	File = {:Users/jialelim/Desktop/CRISPR-Cas/Papers/CrisprGE, a central hub of CRISPR-Cas-based genome editing.pdf:pdf},
