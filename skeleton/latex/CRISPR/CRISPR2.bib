	Pmid = {26782639},
	Publisher = {Official journal of the American Society of Gene {\&} Cell Therapy},
	Shorttitle = {Mol Ther},
	Title = {{The \textit{Neisseria meningitidis} CRISPR-Cas9 System Enables Specific Genome Editing in Mammalian Cells.}},
	Url = {http://dx.doi.org/10.1038/mt.2016.8},
	Volume = {24},
	Year = {2016},
