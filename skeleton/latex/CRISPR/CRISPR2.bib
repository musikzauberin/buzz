	Arxivid = {NIHMS150003},
	Author = {Wu, X and Scott, D A and Kriz, A J and Chiu, A C and Hsu, P D and Dadon, D B and Cheng, A W and Trevino, A E and Konermann, S and Chen, S and Jaenisch, R and Zhang, F and Sharp, P A},
