@article{Pal2014,
	Abstract = {Genome engineering strategies - such as genome editing, reduction and shuffling, and de novo genome synthesis - enable the modification of specific genomic locations in a directed and combinatorial manner. These approaches offer an unprecedented opportunity to study central evolutionary issues in which natural genetic variation is limited or biased, which sheds light on the evolutionary forces driving complex and extremely slowly evolving traits; the selective constraints on genome architecture; and the reconstruction of ancestral states of cellular structures and networks.},
