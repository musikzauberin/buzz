	Keywords = {alkaline phosphatase,cell culture,culture media,human,osteoblasts,osteogenic differentiation,osteogenic supplements,von Kossa staining},
	Month = {apr},
	Number = {3},
	Pages = {161--169},
	Title = {{Biotechnic {\&} Histochemistry Osteogenic differentiation of immature osteoblasts: Interplay of cell culture media and supplements Osteogenic differentiation of immature osteoblasts: Interplay of cell culture media and supplements}},
