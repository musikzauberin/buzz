	Keywords = {Bone Marrow Cells,Bone Marrow Cells: cytology,Cell Differentiation,Cell Differentiation: physiology,Cell Survival,Cell Survival: physiology,Cells, Cultured,Humans,Immunohistochemistry,Magnetite Nanoparticles,Magnetite Nanoparticles: chemistry,Nanotechnology,Nanotechnology: methods,Oligopeptides,Oligopeptides: chemistry,Potassium Channels, Tandem Pore Domain,Potassium Channels, Tandem Pore Domain: chemistry,Reverse Transcriptase Polymerase Chain Reaction,Stromal Cells,Stromal Cells: cytology},
