	File = {:Users/jialelim/Downloads/oxysterols enhance osteoblast diff in vitro and bone healing in vivo.pdf:pdf},
	Isbn = {0250-7005 (Print)$\backslash$r0250-7005 (Linking)},
	Issn = {02507005},
	Journal = {Anticancer Research},
	Keywords = {Cellular proliferation,Normal human urothelial cultures,Radiation,c-myc oncoprotein},
	Number = {4},
	Pages = {1609--1612},
	Pmid = {1746918},
	Title = {{Induction of c-myc oncoprotein and of cellular proliferation by radiation in normal human urothelial cultures}},
	Volume = {11},
	Year = {1991},
	Bdsk-Url-1 = {http://dx.doi.org/10.1002/jor}}

